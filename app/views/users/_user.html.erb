<div class="col-3">
  <div class="card">
  <%= image_tag(user.profile_picture) %>
  <div class="card-body">
    <h3 class="card-title text-center"><%= user.username %></h5>
    <p class="card-text"><%= user.country %>
    <p class="card-text">$<%= user.rates %>/hr</p>
    <a href=<%="/#{user.username.gsub(" ","")}"%> class="btn btn-primary">></a>
  </div>
  </div>
</div>
